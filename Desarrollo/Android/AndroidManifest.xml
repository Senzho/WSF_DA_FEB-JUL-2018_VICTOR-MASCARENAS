    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".InicioSesionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Solicitante.ConsultarPerfilSolicitanteActivity" />
        <activity android:name=".Solicitante.BuscarTrabajadorActivity" />
        <activity android:name=".Solicitante.DatosTrabajadorActivity" />
        <activity android:name=".Solicitante.EnviarPeticionActivity" />
        <activity android:name=".Solicitante.PuntuarTrabajadorActivity" />
        <activity android:name=".PrestadorServicios.ConsultarPerfilPrestadorActivity" />
        <activity android:name=".PrestadorServicios.ConsultarSolicitudesActivity" />
        <activity android:name=".PrestadorServicios.ResponderSolicitudActivity"></activity>
    </application>

</manifest>